<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('head') %> 
    <link rel="stylesheet" href="/styles/booksStyle.css">
</head>
<body>
   <%- include('navbar') %>
   
   <main>
    <section>
        <div id="search-bar">
            <form action="/searchResults" method="post" class="d-flex">
              <input
                class="form-control me-2"
                type="search"
                placeholder="Search"
                aria-label="Search"
                name="searchResult"
              />
              <button class="btn btn-outline-success" type="submit">
                Search
              </button>
            </form>
          </div>
    </section>
    <div id="books-position" >
        
        <div class="card bookInfo" style=" width: 30rem; height: 100%; text-align: center;">
            <div class="info-image-holder">
                <img
                    src="data:image/png;base64,"
                    class="card-img-top"
                    style="width: 100%; height: 100%;"
                    alt="..."
                />
            </div>
            <div class="card-body">
                <h5 class="card-title"><%= book.title %> </h5>
                <p class="card-text"><%= book.name  %> </p>
                <p class="card-text"><%= book.summary %> </p>
                <div >
                    <form action="/deleteBook/<%= book._id %>" method="post">
                        <button class="button-links btn btn-primary">Delete Book</button>
                    </form>
                    <div class="button-links"><a href="/editBook/<%= book._id %> " class="btn btn-primary"> Edit Book</a></div>
                </div>
            </div>
            </div>
        
    </div>
   </main>
</body>
</html>